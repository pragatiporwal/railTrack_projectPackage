<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_twentyfour"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/rest5"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.dell.railtrack.twentyfour"
    tools:showIn="@layout/app_bar_twentyfour">

    <TextView
        android:text="Food number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="61dp"
        android:id="@+id/textView38"
        android:textColor="#A93226"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="19dp" />

    <TextView
        android:text="Food Item Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        android:id="@+id/textView41"
        android:textColor="#A93226"
        android:layout_below="@+id/textView38"
        android:layout_alignStart="@+id/textView38"
        android:layout_alignLeft="@+id/textView38" />

    <TextView
        android:text="Price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView41"
        android:layout_alignStart="@+id/textView41"
        android:layout_marginTop="29dp"
        android:textColor="#A93226"
        android:id="@+id/textView42"
        android:layout_alignLeft="@+id/textView41" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:textColor="#A93226"
        android:layout_alignBottom="@+id/textView41"
        android:layout_toEndOf="@+id/textView41"
        android:layout_marginStart="34dp"
        android:layout_marginBottom="30dp"
        android:id="@+id/editText16"
        android:layout_toRightOf="@+id/textView41"
        android:layout_marginLeft="34dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:textColor="#A93226"
        android:ems="10"
        android:id="@+id/editText17"
        android:layout_alignBaseline="@+id/textView41"
        android:layout_alignBottom="@+id/textView41"
        android:layout_alignStart="@+id/editText16"
        android:layout_alignLeft="@+id/editText16" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:textColor="#A93226"
        android:ems="10"
        android:layout_below="@+id/editText17"
        android:layout_alignStart="@+id/editText17"
        android:id="@+id/editText18"
        android:layout_alignLeft="@+id/editText17" />
<TextView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:textColor="#FF0000"
    android:id="@+id/tvfill"/>
    <Button
        android:text="Ok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editText18"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="58dp"
        android:id="@+id/button24" />
</RelativeLayout>
