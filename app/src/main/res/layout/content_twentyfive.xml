<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_twentyfive"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/rest5"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.dell.railtrack.twentyfive"
    tools:showIn="@layout/app_bar_twentyfive">

    <TextView
        android:text="Enter food number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:textColor="#A93226"
        android:layout_marginStart="14dp"
        android:layout_marginTop="72dp"
        android:id="@+id/textView43"
        android:layout_marginLeft="14dp"
        android:layout_alignParentLeft="true" />

    <Button
        android:text="Delete food item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView43"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="100dp"
        android:id="@+id/btndelete" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:textColor="#A93226"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/textView43"
        android:layout_marginStart="30dp"
        android:layout_marginTop="55dp"
        android:id="@+id/etno"
        android:layout_toRightOf="@+id/textView43"
        android:layout_marginLeft="30dp" />
</RelativeLayout>
