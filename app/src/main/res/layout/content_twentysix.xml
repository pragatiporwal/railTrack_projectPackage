<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_twentysix"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:background="@drawable/rest5"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.dell.railtrack.twentysix"
    tools:showIn="@layout/app_bar_twentysix">

    <TextView
        android:text="Enter food Item number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="85dp"
        android:id="@+id/textView44"
        android:textColor="#641E16"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="19dp" />

    <TextView
        android:text="Enter new Price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView44"
        android:layout_alignStart="@+id/textView44"
        android:layout_marginTop="59dp"
        android:textColor="#641E16"
        android:id="@+id/textView45"
        android:layout_alignLeft="@+id/textView44" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:layout_alignBottom="@+id/textView45"
        android:layout_toEndOf="@+id/textView44"
        android:layout_marginStart="28dp"
        android:textColor="#641E16"
        android:layout_marginBottom="63dp"
        android:id="@+id/editText22"
        android:layout_marginLeft="28dp"
        android:layout_toRightOf="@+id/textView44" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:layout_alignBottom="@+id/textView45"
        android:layout_alignStart="@+id/editText22"
        android:textColor="#641E16"
        android:id="@+id/editText23"
        android:layout_alignLeft="@+id/editText22" />

    <Button
        android:text="Update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editText22"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="78dp"
        android:id="@+id/button26" />
</RelativeLayout>
